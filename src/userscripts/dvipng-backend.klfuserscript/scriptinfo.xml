<?xml version="1.0"?>
<klfuserscript-info klf-compat-version="4">
  <exe-script>dvipng-backend.py</exe-script>
  <can-provide-default-settings>true</can-provide-default-settings>
  <category>klf-backend-engine</category>
  <name>Direct PNG generation with DVIPNG</name>
  <author>Philippe Faist, philippe.faist@bluewin.ch</author>
  <version>0.1</version>
  <license>GPL v2+</license>
  <settings-form-ui>dvipng_config.ui</settings-form-ui>
  <klf-backend-engine>
    <spits-out>png</spits-out>
    <skip-formats selector="all-except">
      dvi
      png
    </skip-formats>
  </klf-backend-engine>
</klfuserscript-info>
